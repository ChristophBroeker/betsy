<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
   <bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="WCP10ArbitraryCycleMMTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/wCP10ArbitraryCycleMM">
    <bpmn2:process id="WCP10ArbitraryCycleMM" isExecutable="true"> 
	
	
	
	<bpmn2:startEvent id="Start" name="Start">
	<bpmn2:outgoing>Flow1</bpmn2:outgoing>
	</bpmn2:startEvent>
	
    <bpmn2:scriptTask id="SET_COUNTER" name="SET_COUNTER">
	<bpmn2:incoming>Flow1</bpmn2:incoming>
	<bpmn2:outgoing>Flow2</bpmn2:outgoing>
	<bpmn2:script>SET_COUNTER</bpmn2:script>
	</bpmn2:scriptTask>
	
    <bpmn2:parallelGateway gatewayDirection="Diverging" id="Parallel1" name="ParallelGateway">
	<bpmn2:incoming>Flow2</bpmn2:incoming>
	<bpmn2:outgoing>Flow3</bpmn2:outgoing>
	<bpmn2:outgoing>Flow4</bpmn2:outgoing>
	</bpmn2:parallelGateway>
	
    <bpmn2:exclusiveGateway gatewayDirection="Converging" id="Ex1" name="ExclusiveGateway">
	<bpmn2:incoming>Flow4</bpmn2:incoming>
    <bpmn2:incoming>Flow9</bpmn2:incoming>
	<bpmn2:outgoing>Flow5</bpmn2:outgoing>
	</bpmn2:exclusiveGateway>
	
	<bpmn2:scriptTask id="Task2" name="ScriptTask2">
	<bpmn2:incoming>Flow5</bpmn2:incoming>
	<bpmn2:outgoing>Flow6</bpmn2:outgoing>
	<bpmn2:script>SCRIPT_task2</bpmn2:script>
	</bpmn2:scriptTask>
	
	
    <bpmn2:scriptTask id="Task5" name="ScriptTask5">
	<bpmn2:incoming>Flow3</bpmn2:incoming>
	<bpmn2:outgoing>Flow7</bpmn2:outgoing>
	<bpmn2:script>SCRIPT_task5</bpmn2:script>
	</bpmn2:scriptTask>
	
	
    <bpmn2:scriptTask id="Task3" name="ScriptTask3">
	<bpmn2:incoming>Flow6</bpmn2:incoming>
	<bpmn2:incoming>Flow7</bpmn2:incoming>
	<bpmn2:outgoing>Flow8</bpmn2:outgoing>
	<bpmn2:script>INC_COUNTER</bpmn2:script>
	</bpmn2:scriptTask>
	
    <bpmn2:exclusiveGateway gatewayDirection="Diverging" id="EX2" name="ExclusiveGateway">
	<bpmn2:incoming>Flow8</bpmn2:incoming>
	<bpmn2:outgoing>Flow9</bpmn2:outgoing>
	<bpmn2:outgoing>Flow10</bpmn2:outgoing>
	</bpmn2:exclusiveGateway>
	
    <bpmn2:scriptTask id="Task4" name="ScriptTask4">
	<bpmn2:incoming>Flow10</bpmn2:incoming>
	<bpmn2:outgoing>Flow11</bpmn2:outgoing>
	<bpmn2:script>SCRIPT_task4</bpmn2:script>
	</bpmn2:scriptTask>
	
    <bpmn2:endEvent id="End" name="EndEvent">
	<bpmn2:incoming>Flow11</bpmn2:incoming>
	</bpmn2:endEvent>
	
	
    <bpmn2:sequenceFlow id="Flow1" sourceRef="Start" targetRef="SET_COUNTER"/>
    <bpmn2:sequenceFlow id="Flow2" sourceRef="SET_COUNTER" targetRef="Parallel1"/>
    <bpmn2:sequenceFlow id="Flow8" sourceRef="Task3" targetRef="EX2"/>
    <bpmn2:sequenceFlow id="Flow3" sourceRef="Parallel1" targetRef="Task2"/>
    <bpmn2:sequenceFlow id="Flow4" sourceRef="Parallel1" targetRef="Ex1"/>
    <bpmn2:sequenceFlow id="Flow5" sourceRef="Ex1" targetRef="Task5"/>
    <bpmn2:sequenceFlow id="Flow6" sourceRef="Task2" targetRef="Task3"/>
    <bpmn2:sequenceFlow id="Flow7" sourceRef="Task5" targetRef="Task3"/>
	
    <bpmn2:sequenceFlow id="Flow9" sourceRef="EX2" targetRef="Ex1">
	<bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">counter != 5</bpmn2:conditionExpression>
	</bpmn2:sequenceFlow>
	
    <bpmn2:sequenceFlow id="Flow10" sourceRef="EX2" targetRef="Task4">
	<bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">counter == 5</bpmn2:conditionExpression>
	</bpmn2:sequenceFlow>
	
    <bpmn2:sequenceFlow id="Flow11" sourceRef="Task4" targetRef="End"/>
  </bpmn2:process>
  
</bpmn2:definitions>

	<bpmn2:incoming>_15</bpmn2:incoming>
	<bpmn2:incoming>_6</bpmn2:incoming>
	<bpmn2:outgoing>_8</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
	
	<bpmn2:scriptTask id="ScriptTask2" name="IncCounter">
	<bpmn2:incoming>_8</bpmn2:incoming>
	<bpmn2:outgoing>_10</bpmn2:outgoing>
	<bpmn2:script>INC_COUNTER</bpmn2:script>
	</bpmn2:scriptTask>
	
    <bpmn2:scriptTask id="ScriptTask3" name="ScriptTask1">
	<bpmn2:incoming>_10</bpmn2:incoming>
	<bpmn2:outgoing>_12</bpmn2:outgoing>
	<bpmn2:script>SCRIPT_task1</bpmn2:script>
	</bpmn2:scriptTask>
	
    <bpmn2:exclusiveGateway gatewayDirection="Diverging" default="_17" id="ExclusiveGateway2" name="ExclusiveGateway2">
	<bpmn2:incoming>_12</bpmn2:incoming>
	<bpmn2:outgoing>_17</bpmn2:outgoing>
	<bpmn2:outgoing>_15</bpmn2:outgoing>

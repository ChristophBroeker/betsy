                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="WCP10ArbitraryCycleTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/wCP10ArbitraryCycle">
    <bpmn2:process id="WCP10ArbitraryCycle" isExecutable="true"> 	
	
	
	<bpmn2:startEvent id="Start" name="Start">

	<bpmn2:sequenceFlow id="_6" name="Flow2" sourceRef="ScriptTask1" targetRef="ExclusiveGateway1"/>
    <bpmn2:sequenceFlow id="_8" name="Flow3" sourceRef="ExclusiveGateway1" targetRef="ScriptTask2"/>
    <bpmn2:sequenceFlow id="_10" name="Flow4" sourceRef="ScriptTask2" targetRef="ScriptTask3"/>
    <bpmn2:sequenceFlow id="_12" name="Flow5" sourceRef="ScriptTask3" targetRef="ExclusiveGateway2"/>
    <bpmn2:sequenceFlow id="_19" name="Flow8" sourceRef="ScriptTask4" targetRef="End"/>
